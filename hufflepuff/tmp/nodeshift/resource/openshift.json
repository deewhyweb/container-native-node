{
  "apiVersion": "v1",
  "kind": "List",
  "items": [
    {
      "apiVersion": "v1",
      "kind": "DeploymentConfig",
      "metadata": {
        "name": "hufflepuff",
        "namespace": "hogwarts",
        "labels": {
          "project": "hufflepuff",
          "version": "1.0.0",
          "provider": "nodeshift",
          "app": "hufflepuff"
        },
        "annotations": {
          "nodeshift/git-branch": "demojam",
          "nodeshift/git-commit": "074a9e9cb590c4c41d74f69313041cb1ee018525"
        }
      },
      "spec": {
        "replicas": 1,
        "revisionHistoryLimit": 2,
        "selector": {
          "app": "hufflepuff",
          "project": "hufflepuff",
          "provider": "nodeshift"
        },
        "triggers": [
          {
            "type": "ConfigChange"
          },
          {
            "type": "ImageChange",
            "imageChangeParams": {
              "automatic": true,
              "containerNames": [
                "hufflepuff"
              ],
              "from": {
                "kind": "ImageStreamTag",
                "name": "hufflepuff:latest"
              }
            }
          }
        ],
        "template": {
          "metadata": {
            "labels": {
              "project": "hufflepuff",
              "version": "1.0.0",
              "provider": "nodeshift",
              "app": "hufflepuff",
              "name": "hufflepuff"
            },
            "annotations": {
              "sidecar.istio.io/inject": "true",
              "nodeshift/git-branch": "demojam",
              "nodeshift/git-commit": "074a9e9cb590c4c41d74f69313041cb1ee018525"
            }
          },
          "spec": {
            "containers": [
              {
                "readinessProbe": {
                  "httpGet": {
                    "path": "/api/health/readiness",
                    "scheme": "HTTP",
                    "port": 3000
                  },
                  "failureThreshold": 3,
                  "initialDelaySeconds": 10,
                  "periodSeconds": 5,
                  "successThreshold": 1,
                  "timeoutSeconds": 1
                },
                "livenessProbe": {
                  "httpGet": {
                    "path": "/api/health/liveness",
                    "scheme": "HTTP",
                    "port": 3000
                  },
                  "initialDelaySeconds": 60,
                  "periodSeconds": 3,
                  "failureThreshold": 2,
                  "successThreshold": 1,
                  "timeoutSeconds": 1
                },
                "image": "hufflepuff",
                "name": "hufflepuff",
                "securityContext": {
                  "privileged": false
                },
                "ports": [
                  {
                    "containerPort": 8080,
                    "name": "http",
                    "protocol": "TCP"
                  }
                ],
                "env": null,
                "resources": null,
                "limits": {
                  "cpu": "200m",
                  "memory": "400Mi"
                },
                "requests": {
                  "cpu": "100m",
                  "memory": "200Mi"
                }
              }
            ]
          }
        }
      }
    },
    {
      "apiVersion": "v1",
      "kind": "Service",
      "spec": {
        "selector": {
          "project": "hufflepuff",
          "provider": "nodeshift"
        },
        "ports": [
          {
            "protocol": "TCP",
            "port": 8080,
            "targetPort": 8080,
            "name": "http"
          }
        ],
        "type": "ClusterIP"
      },
      "metadata": {
        "name": "hufflepuff",
        "namespace": "hogwarts",
        "labels": {
          "project": "hufflepuff",
          "version": "1.0.0",
          "provider": "nodeshift"
        },
        "annotations": {
          "nodeshift/git-branch": "demojam",
          "nodeshift/git-commit": "074a9e9cb590c4c41d74f69313041cb1ee018525"
        }
      }
    }
  ]
}