  "items": [
    {
      "apiVersion": "v1",
      "kind": "DeploymentConfig",
      "metadata": {
        "name": "catalog",
        "namespace": "samplenode",
        "creationTimestamp": null,
        "labels": {
          "project": "catalog",
          "version": "1.0.0",
          "provider": "nodeshift",
          "app": "catalog"
        },
        "annotations": {
          "nodeshift/git-branch": "master",
          "nodeshift/git-commit": "67888f223cc47667f375995af334930814e3fa35"
        }
      },
      "spec": {
        "replicas": 1,
        "revisionHistoryLimit": 2,
        "selector": {
          "app": "catalog",
          "project": "catalog",
          "provider": "nodeshift"
        },
        "triggers": [
          {
            "type": "ConfigChange"
